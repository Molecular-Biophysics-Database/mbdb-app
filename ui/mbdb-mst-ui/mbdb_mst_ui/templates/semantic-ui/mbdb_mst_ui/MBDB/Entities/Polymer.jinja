{# def entity #}

<MBDB.NameValue.Item name="Organism">{{ entity.organism }}</MBDB.NameValue.Item>
<MBDB.NameValue.Item name="Sequence">{{ entity.sequence }}</MBDB.NameValue.Item>
<MBDB.NameValue.Item name="Modifications">
  {% if entity.modifications is defined %}
    <MBDB.Entities.Polymer.Modifications modifications={{ entity.modifications }} />
  {% endif %}
</MBDB.NameValue.Item>
