{# def gp #}

<IValueList>
    {%- for dp in array(gp.results) %}
    {% set title='Result ' ~ loop.index %}
    <div class="mb-4 pb-8 border-b-[.1px] border-b-dark last:border-b-0 last:mb-0">
        <SubSection title={ title }>
            <IField label={ dp.name }>
                <MeasuredValue value={dp} />
            </IField>
            <IField label="Result type" d={dp.type} />
        </SubSection>
    </div>
    {% endfor %}
</IValueList>